@model Compilify.Models.Document

<li>
    <span class="tab" data-toggle="tab" data-target="#@Model.Name" data-name="@Model.Name">
        <span class="tab-name">@Model.Name</span>
        <span class="dropdown">
            <a class="dropdown-toggle" data-toggle="dropdown">
                <i class="icon-chevron-down"></i>
            </a>
            <ul class="dropdown-menu">
                <li>
                    <a class="js-rename-tab" href="#">
                        <i class="icon-pencil"></i>
                        Rename
                    </a>
                </li>
                <li class="divider"></li>
                <li>
                    <a class="js-delete-tab" href="#">
                        <i class="icon-trash"></i>
                        Delete
                    </a>
                </li>
            </ul>
        </span>
    </span>
</li>